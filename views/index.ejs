  <!-- ======= Property Grid ======= -->
  <section class="property-grid grid">
    <div class="container">
      <div class="row">
    <!-- BEGIN DYNAMIC CARD. -->
    <% homes.forEach(home => { %>
        <div class="col-md-4">
          <div class="card-box-a card-shadow">
            <div class="img-box-a image-parent">
              <img src="<%= home.homeImgPathURL %>">
            </div>
            <div class="card-overlay">
              <div class="card-overlay-a-content">
                <div class="card-header-a">
                  <h2 class="card-title-a">
                    <a href="/<%= home._id %>"><%= home.houseAddress.street %> </a>
                  </h2>
                </div> 
                <div class="card-body-a">
                  <div class="price-box d-flex">
                    <%
                    function formatToDollar(number) {
                      // Convert number to fixed-point notation with 2 decimal places
                      let formattedNumber = Number(number).toFixed(2);
                      // Remove trailing zeros after the decimal point
                      formattedNumber = formattedNumber.replace(/(\.\d*?[1-9])0+$/, '$1');
                      // Add commas for thousands separator
                      formattedNumber = formattedNumber.replace(/\d(?=(\d{3})+\.)/g, '$&,');
                      // Add currency symbol
                      return '$' + formattedNumber;
                    }
                  %>
                  
                  
                    <span class="price-a">sale | $  <%= home.price %>  </span>
                  </div>
                  <a href="/<%= home._id %>">View Property</a>
                    <span class="bi bi-chevron-right"></span>
                  </a>
                </div>
          
              </div>
            </div>
          </div>
        </div>
      <% }); %>
      


<!-- END DYNAMIC EJS CARD FOR PROPERTY GRID-->


     
      </div>
      
    <!-- END DYNAMIC CARD. -->
